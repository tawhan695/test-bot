nano bot.sh
"
#!/bin/bash
cd ~
./main token5

"

nano /etc/systemd/system/botrunner.service
"
[Unit]
Description=botline
After=network.target

[Service]
ExecStart=/root/bot.sh
Restart=always
User=root
Group=root

[Install]
WantedBy=multi-user.target
"

#setup

cp botService.service /etc/systemd/system/botService.service
chmod +x bot.sh
systemctl enamble botrunner.service
systemctl start botrunner.service
systemctl status botrunner.service